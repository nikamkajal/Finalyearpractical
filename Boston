import tensorflow as tf 
import pandas as pd 
import numpy as pd



from tensorflow.keras.datasets import boston_housing

(train_x,train_y ),(test_x,test_y)=boston_housing.load_data()

print (' train shape : ', train_x.shape) 
print (' train shape : ', test_x.shape) 
print (' training shample : ', train_x[0]) 
print (' training target : ', train_y[0])



mean = train_x.mean(axis=0) 
std = train_x.std(axis=0)

train_x = (train_x - mean)/std
test_x = (test_x - mean)/std

train_x[0]

from tensorflow.keras.models import Sequential 
from tensorflow.keras.layers import Dense

def HPPM():
  model=Sequential() 
  model.add(Dense(128,activation='relu',input_shape=(train_x[0].shape)))
  model.add(Dense(64,activation='relu',name='dense_2')) 
  model.add(Dense(1,activation='linear',name='dense_output')) 
  model.compile(optimizer='adam', loss='mse',metrics=['mse']) 
  model.summary()
  return model

model = HPPM()
history =model.fit(x=train_x,y=train_y,epochs=100,batch_size=1, verbose = 1,validation_steps = (train_x,train_y))

test_x[7]

print('actual output : ',test_y[7])
